<form method="post" action="{{ path('app_page_delete', {'id': page.id}) }}" onsubmit="return confirm('Voulez-vous vraiment supprimer cette page ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ page.id) }}">
    <button class="btn btn-danger btn-sm mt-2"><i class="fa-solid fa-trash"></i></button>
</form>
